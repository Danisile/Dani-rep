@model IEnumerable<CarParkingApp.Models.Parking>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Parking_Type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Slots)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Park_Space)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Parking_Type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Slots)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Park_Space)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Parking_Num }) |
            @Html.ActionLink("Details", "Details", new { id=item.Parking_Num }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Parking_Num })
        </td>
    </tr>
}

</table>
